ue,\r\n        },\r\n      });\r\n\r\n    // history.replace('/orders')\r\n    history.push(\"./payment\");\r\n  };\r\n\r\n  //console.log(\"here is \", userData);\r\n\r\n  return (\r\n    <div className=\"delivery\">\r\n      <Progressbar status=\"2\" />\r\n\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"card d-flex justify-content-center mx-auto my-3 p-5\">\r\n            <h2>Add Your Shipping Details</h2>\r\n\r\n            <form onSubmit={handleSubmit}>\r\n              <div class=\"form-row\">\r\n                <div class=\"form-group col-md-12 first\">\r\n                  {\" \"}\r\n                  <label for=\"inputName\" className=\"heading\">\r\n                    NAME <span>*</span>\r\n                  </label>{\" \"}\r\n                  <input\r\n                    name=\"name\"\r\n                    id=\"inputName\"\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    placeholder=\"Enter your name\"\r\n                    defaultValue={userData?.name}\r\n                    required\r\n                  />\r\n                </div>\r\n              </div>\r\n\r\n              \r\n                <label for=\"inputArea\" className=\"heading del\">\r\n                  DELIVERY ADDRESS\r\n                </label>\r\n              \r\n\r\n              <div class=\"form-row\">\r\n                <div class=\"form-group col-md-6 pt-0\">\r\n                  {\" \"}\r\n                  <label for=\"inputArea\">\r\n                    AREA <span>*</span>\r\n                  </label>{\" \"}\r\n                  <input\r\n                    name=\"area\"\r\n                    id=\"inputArea\"\r\n                    placeholder=\"Enter your area\"\r\n                    defaultValue={userData?.address?.area}\r\n                    class=\"form-control\"\r\n                    required\r\n                  />\r\n                </div>\r\n                <div class=\"form-group col-md-6 pt-0\">\r\n                  {\" \"}\r\n                  <label for=\"inputCity\">\r\n                    CITY <span>*</span>\r\n                  </label>{\" \"}\r\n                  <input\r\n                    name=\"city\"\r\n                    id=\"inputCity\"\r\n                    placeholder=\"Enter your city\"\r\n                    defaultValue={userData?.address?.city}\r\n                    class=\"form-control\"\r\n                    required\r\n                  />\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"form-row\">\r\n                <div class=\"form-group col-md-6\">\r\n                  {\" \"}\r\n                  <label for=\"inputState\">\r\n                    STATE <span>*</span>\r\n                  </label>{\" \"}\r\n                  <input\r\n                    name=\"state\"\r\n                    id=\"inputState\"\r\n                    placeholder=\"Enter your state\"\r\n                    defaultValue={userData?.address?.state}\r\n                    class=\"form-control\"\r\n                    required\r\n                  />\r\n                </div>\r\n                <div class=\"form-group col-md-6\">\r\n                  {\" \"}\r\n                  <label for=\"inputCountry\">\r\n                    COUNTRY <span>*</span>\r\n                  </label>{\" \"}\r\n                  <input\r\n                    name=\"country\"\r\n                    id=\"inputCountry\"\r\n                    placeholder=\"Enter your country\"\r\n                    defaultValue={userData?.address?.country}\r\n                    class=\"form-control\"\r\n                    required\r\n                  />\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"form-row\">\r\n                <div class=\"form-group col-md-6\">\r\n                  {\" \"}\r\n                  <label for=\"inputPincode\">\r\n                    PINCODE <span>*</span>\r\n                  </label>{\" \"}\r\n                  <input\r\n                    name=\"pincode\"\r\n                    placeholder=\"Enter your pincode\"\r\n                    defaultValue={userData?.address?.pincode}\r\n                    id=\"inputPincode\"\r\n                    class=\"form-control\"\r\n                    required\r\n                  />\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"form-row\">\r\n                <div class=\"form-group col-md-6\">\r\n                  {\" \"}\r\n                  <label for=\"inputEmail\" className=\"heading\">\r\n                    EMAIL <span>*</span>\r\n                  </label>{\" \"}\r\n                  <input\r\n                    name=\"email\"\r\n                    id=\"inputEmail\"\r\n                    type=\"email\"\r\n                    class=\"form-control\"\r\n                    placeholder=\"Enter your email\"\r\n                    defaultValue={userData?.email}\r\n                    required\r\n                  />\r\n                  <div id=\"email_error\" class=\"val_error\"></div>\r\n                </div>\r\n                <div class=\"form-group col-md-6\">\r\n                  {\" \"}\r\n                  <label for=\"inputPhone\" className=\"heading\">\r\n                    PHONE <span>*</span>\r\n                  </label>{\" \"}\r\n                  <input\r\n                    type=\"tel\"\r\n                    name=\"phone\"\r\n                    id=\"inputPhone\"\r\n                    placeholder=\"Enter your phone\"\r\n                    defaultValue={userData?.phone}\r\n                    class=\"form-control\"\r\n                    required\r\n                  />\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"form-button pt-4\">\r\n                {\" \"}\r\n                <button type=\"submit\" class=\"btn btn-primary btn-block btn-lg\">\r\n                  